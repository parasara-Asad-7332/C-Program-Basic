#include <stdio.h>
int main()
{
    int a = 6; // store number
    // int a ;
    // a = 6 ; same as up

    float b; // store float number like 3.5 , 4.9 . 5.4
    b = 95.5;
    // float b = 95.5; same as up

    char c = 'A'; // Store a character
    // char c;
    // c = 'a'; // same as up

    printf("this viule a %d\n", a);
    printf("this viule b %f\n", b);
    printf("this viule c %c\n", c);
}
